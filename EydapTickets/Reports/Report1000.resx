  <data name="customSqlQuery1.Sql" xml:space="preserve">
    <value>SELECT 
       ID1022, 
       DateOfAssignment, 
	 Municipality,
	 Street_Name, 
	 Street_Number, 
	 Perioxi,
	 SectorId,
	 SectorDescription,
	 DepartmentId,
	 TaskTypeId,
	 Task_Description,
	 State,
	 BackEndTabletId, 
	 ID,
	 EpanaforaOdostromatos_Mikos1,
	 EpanaforaOdostromatos_Platos1,
   	 Mpeton_mikos, 
	 Mpeton_platos,
	 Kraspedo,
	 Reithro,
	 Mikos,
	 ArithmosMetriti,
	 ApokomidiMpazwnEkkremei,
	 Paratiriseis,
	 WorkTeam_ID,
	 FName,
	 LName
	 FROM 
	 (
SELECT 
     A.ID1022, 
     H.Assigned                          AS DateOfAssignment, 
     A.Municipality  COLLATE Greek_CI_AS AS Municipality,
     A.Street_Name   COLLATE Greek_CI_AS As Street_Name, 
     A.Street_Number COLLATE Greek_CI_AS AS Street_Number, 
     A.Perioxi,
     B.SectorId,
     G.SectorDescription,
     B.DepartmentId,
     B.TaskTypeId,
     B.Task_Description,
     B.State,
     B.BackEndTabletId, 
     C.ID,
     D.EpanaforaOdostromatos_Mikos1,
     D.EpanaforaOdostromatos_Platos1,
     E.Mpeton_mikos, 
     E.Mpeton_platos,
     D.Kraspedo,
     D.Reithro,
     D.Mikos,
     A.ArithmosMetriti,
     D.ApokomidiMpazwnEkkremei,
     F.Paratiriseis,
	 (SELECT MAX(A.Workteam_ID)
		FROM [EydapFieldWorks].[dbo].WorkteamIncidents AS A
		WHERE A.Incident_ID = C.ID
	 ) AS WorkTeam_ID,
	 (
		SELECT TOP 1 FirstName
			  FROM [EydapFieldWorks].[dbo].AspNetUsers A, [EydapFieldWorks].[dbo].UsersInWorkTeams  B
			 WHERE B.WorkTeam_ID = (SELECT MAX(Workteam_ID) FROM [EydapFieldWorks].[dbo].WorkteamIncidents WHERE Incident_ID = C.ID)
			   AND B.User_ID = A.Id
	 ) AS FName,
	 (
		SELECT TOP 1 LastName
			  FROM [EydapFieldWorks].[dbo].AspNetUsers A, [EydapFieldWorks].[dbo].UsersInWorkTeams  B
			 WHERE B.WorkTeam_ID = (SELECT MAX(Workteam_ID) FROM [EydapFieldWorks].[dbo].WorkteamIncidents WHERE Incident_ID = C.ID)
			   AND B.User_ID = A.Id
	 ) AS LName
FROM [TT_db].[dbo].Incidents                            A,
     [TT_db].[dbo].Tasks                                B, 
     [EydapFieldWorks].[dbo].Incident                   C,
     [EydapFieldWorks].[dbo].ErgasiesApokatastasisTomis D,
     [EydapFieldWorks].[dbo].ErgasiesEpiskeuhs          E,
     [EydapFieldWorks].[dbo].FinalComments              F,
     [TT_db].[dbo].Sectors                              G,
     [EydapFieldWorks].[dbo].WorkteamIncidents          H
WHERE A.TT_Id                               = B.Incident_Id AND
      A.Municipality  COLLATE Greek_CI_AS   = C.dimos       AND
	A.Street_Name   COLLATE Greek_CI_AS = C.odos1         AND
	A.Street_Number COLLATE Greek_CI_AS = C.StreetNumber1 AND
      G.SectorId                          = B.SectorId      AND 
      B.TaskTypeId                        = 1120            AND              
      B.BackEndTabletId                   = C.ID            AND 
	D.Incident_ID                       = C.ID            AND
	E.Incident_ID                       = C.ID            AND
	F.Incident_ID                       = C.ID            AND
	H.Incident_ID                       = C.ID            AND
	H.Workteam_ID                       = ( SELECT MAX(Workteam_ID)
		                                    FROM [EydapFieldWorks].[dbo].WorkteamIncidents
		                                   WHERE Incident_ID = C.ID ) AND
	B.BackEndTabletId IS NOT NULL     
) RESULTS</value>
  </data>
  <data name="customSqlQuery2.Sql" xml:space="preserve">
    <value>SELECT A.Incident_ID,
	 A.Description, 
	 A.IsCubeStone, 
	 A.IsPlate, 
       A.Items, 
      (CASE A.Size
	        WHEN 0 THEN ''
			WHEN 1 THEN '10X10'
			WHEN 2 THEN '20X10' 
	        	WHEN 4 THEN '40X40'
			WHEN 5 THEN '50X50'
	    		ELSE 'ΑΓΝΩΣΤΟ'
		END) AS TSize,
		A.CubeStonesLength,
		A.CubeStonesWidth,
		(CASE A.Color
	        WHEN 0 THEN ''
			WHEN 1 THEN 'ΑΣΠΡΟ'
			WHEN 2 THEN 'ΚΙΤΡΙΝΟ'
	        	WHEN 3 THEN 'ΓΚΡΙ'
			WHEN 4 THEN 'ΚΟΚΚΙΝΟ'
	    		ELSE 'ΑΓΝΩΣΤΟ'
		END) AS TColor,
	    A.ExistOnSite
	  FROM [EydapFieldWorks].[dbo].IncidentStructureMaterials AS A</value>
  </data>
